    "def find_second_largest(numbers):\n",
    "    numbers.sort()\n",
    "    return numbers[-2]\n",
    "\n",
    "# Example usage:\n",
    "num_list = [int(x) for x in input(\"Enter the number items\").split(\" \")]\n",
    "second_largest_num = find_second_largest(num_list)\n",
    "print(\"The second largest number is:\", second_largest_num)"
